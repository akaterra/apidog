<div
    class="{{#ifCond index '!=' 0}}hidden{{/ifCond}}"
    data-block-ssr-class="{{class}}"
    data-block-ssr-field="{{field}}"
    data-block-ssr-group="{{group}}"
    data-block-ssr-group-variant-index="{{index}}"
    data-block-ssr-selectable="groupVariant"
>
    {{#each groupVariantSection.prop as |prop|}}
        {{#ifCond prop.[0].parent '==' null}}
        <div class="section flex">
            {{#with (first (lookup ../../param prop.[0].list.[0]) (lookup ../../../param prop.[0].list.[0])) as |param|}} <!-- ??? -->
                <label
                    class="input-label width-25"
                    for="{{../../../id}}_{{param.field.name}}_{{../../../class}}"
                    title="{{p.field.name}}"
                >
                    {{param.field.name}}
                </label>
            {{/with}}
            <div class="flex flex-1">
                {{#each prop as |p|}}
                    {{#with (first (lookup ../../../param p.list.[0]) (lookup ../../../../param p.list.[0])) as |param|}} <!-- ??? -->
                        {{#ifCond p.parent '!=' null}}

                        {{else ifCond param.type.modifiers.enum '!=' undefined}}
                            <select
                                class="input-param flex-1{{#if @first}}{{else}} hidden{{/if}}"
                                id="{{../../../../../id}}_{{../../../../../group}}_{{param.field.name}}_{{../../../../../class}}_{{@index}}"
                                name="{{param.field.name}}"
                                data-block-ssr-block-index="{{../../../../../group}}_{{param.field.name}}_{{@index}}"
                                data-block-ssr-class="{{../../../../../class}}"
                                data-block-ssr-field="{{param.field.name}}"
                                data-block-ssr-field-variant-index="{{param.field.name}}_{{@index}}"
                                data-block-ssr-group="{{../../../../../group}}"
                                data-block-ssr-group-variant-index="{{@index}}"
                                data-block-ssr-group-variant-param-index="{{p.list.[0]}}"
                                data-block-ssr-input-global-id="_{{param.field.name}}_{{../../../../../class}}"
                                data-block-ssr-param-type="{{param.type.modifiers.initial}}"
                                data-block-ssr-selectable="groupVariant"
                                value="{{param.field.defaultValue}}"
                            >{{#each param.type.allowedValues}}<option value="{{this}}">{{this}}</option>{{/each}}</select>
                        {{else ifCond param.type.modifiers.initial '==' 'parametrizedbody'}}
                            <textarea
                                class="input-param-raw-body flex-1{{#if @first}}{{else}} hidden{{/if}}"
                                id="{{../../../../../id}}_{{../../../../../group}}_{{param.field.name}}_{{../../../../../class}}_{{@index}}"
                                name="{{param.field.name}}"
                                data-block-ssr-block-index="{{../../../../../group}}_{{param.field.name}}_{{@index}}"
                                data-block-ssr-class="{{../../../../../class}}"
                                data-block-ssr-field="{{param.field.name}}"
                                data-block-ssr-field-variant-index="{{param.field.name}}_{{@index}}"
                                data-block-ssr-group="{{../../../../../group}}"
                                data-block-ssr-group-variant-index="{{@index}}"
                                data-block-ssr-group-variant-param-index="{{p.list.[0]}}"
                                data-block-ssr-input-global-id="_{{param.field.name}}_{{../../../../../class}}"
                                data-block-ssr-param-type="{{param.type.modifiers.initial}}"
                                data-block-ssr-selectable="groupVariant"
                                rows="10"
                            >{{param.field.defaultValue}}</textarea>
                        {{else ifCond param.type.modifiers.initial '==' 'rawbody'}}
                            <textarea
                                class="input-param-raw-body flex-1{{#if @first}}{{else}} hidden{{/if}}"
                                id="{{../../../../../id}}_{{../../../../../group}}_{{param.field.name}}_{{../../../../../class}}_{{@index}}"
                                name="{{param.field.name}}"
                                data-block-ssr-block-index="{{../../../../../group}}_{{param.field.name}}_{{@index}}"
                                data-block-ssr-class="{{../../../../../class}}"
                                data-block-ssr-field="{{param.field.name}}"
                                data-block-ssr-field-variant-index="{{param.field.name}}_{{@index}}"
                                data-block-ssr-group="{{../../../../../group}}"
                                data-block-ssr-group-variant-index="{{@index}}"
                                data-block-ssr-group-variant-param-index="{{p.list.[0]}}"
                                data-block-ssr-input-global-id="{{param.field.name}}_{{../../../../../class}}"
                                data-block-ssr-param-type="{{param.type.modifiers.initial}}"
                                data-block-ssr-selectable="groupVariant"
                                rows="10"
                            >{{param.field.defaultValue}}
                            </textarea>
                        {{else ifCond param.type.modifiers.initial '==' 'boolean'}}
                            <input
                                class="input-param flex-1{{#if @first}}{{else}} hidden{{/if}}"
                                id="{{../../../../../id}}_{{../../../../../group}}_{{param.field.name}}_{{../../../../../class}}_{{@index}}"
                                name="{{param.field.name}}"
                                type="checkbox"
                                data-block-ssr-block-index="{{../../../../../group}}_{{param.field.name}}_{{@index}}"
                                data-block-ssr-class="{{../../../../../class}}"
                                data-block-ssr-field="{{param.field.name}}"
                                data-block-ssr-group="{{../../../../../group}}"
                                data-block-ssr-group-variant-index="{{@index}}"
                                data-block-ssr-group-variant-param-index="{{p.list.[0]}}"
                                data-block-ssr-input-global-id="{{param.field.name}}_{{../../../../../class}}"
                                data-block-ssr-param-type="{{param.type.modifiers.initial}}"
                                data-block-ssr-selectable="groupVariant"
                                {{#if param.field.defaultValue}}checked{{/if}}
                            />
                        {{else ifCond param.type.modifiers.initial '==' 'null'}}

                        {{else ifCond (hook 'isNotContainerType' param.type) '!=' true}}

                        {{else}}
                            <input
                                class="input-param flex-1{{#if @first}}{{else}} hidden{{/if}}"
                                id="{{../../../../../id}}_{{../../../../../group}}_{{param.field.name}}_{{../../../../../class}}_{{@index}}"
                                name="{{param.field.name}}"
                                type="{{#ifCond param.type.modifiers.initial '==' 'file'}}file{{else}}text{{/ifCond}}"
                                data-block-ssr-block-index="{{../../../../../group}}_{{param.field.name}}_{{@index}}"
                                data-block-ssr-class="{{../../../../../class}}"
                                data-block-ssr-field="{{param.field.name}}"
                                data-block-ssr-group="{{../../../../../group}}"
                                data-block-ssr-group-variant-index="{{@index}}"
                                data-block-ssr-group-variant-param-index="{{p.list.[0]}}"
                                data-block-ssr-input-global-id="{{param.field.name}}_{{../../../../../class}}"
                                data-block-ssr-param-type="{{param.type.modifiers.initial}}"
                                data-block-ssr-selectable="groupVariant"
                                value="{{param.field.defaultValue}}"
                            />
                        {{/ifCond}}
                    {{/with}}
                {{/each}}
            </div>
            {{#each prop as |p|}}
                {{#with (first (lookup ../../../param p.list.[0]) (lookup ../../../../param p.list.[0])) as |param|}} <!-- ??? -->
                    <span class="input-placeholder mono">{{#if (first (ifCond prop.length '!=' 1) (ifCond (hook 'isGroupVariantOptional' prop ../../../../param) '==' true))}}<input
                        type="radio"
                        name="{{../../../../id}}_{{../../../../group}}_{{param.field.name}}_{{../../../../class}}_gvs"
                        {{#if @first}}checked{{/if}}
                        data-block-ssr-block-index="{{../../../../group}}_{{param.field.name}}_gvs"
                        data-block-ssr-class="{{../../../../class}}"
                        data-block-ssr-field="{{param.field.name}}"
                        data-block-ssr-group="{{../../../../group}}"
                        data-block-ssr-group-variant-index="{{@index}}"
                        data-block-ssr-group-variant-selector="{{param.field.name}}_{{@index}}_{{../../../../class}}"
                    />{{/if}}{{param.type.name}}</span>
                {{/with}}
            {{/each}}
            {{#with (first (lookup (hook 'paramByGroupVariantIndex' prop ../../param 0)) (lookup (hook 'paramByGroupVariantIndex' prop ../../../param 0))) as |param|}} <!-- ??? -->
                {{#ifCond (hook 'isGroupVariantOptional' prop ../../../param) '==' true}}<span class="input-placeholder mono"><input
                    type="radio"
                    name="{{../../../../id}}_{{../../../../group}}_{{param.field.name}}_{{../../../../class}}_gvs"
                    {{!-- {{#if @first}}checked{{/if}} --}}
                    data-block-ssr-block-index="{{../../../../group}}_{{param.field.name}}_gvs"
                    data-block-ssr-class="{{../../../../class}}"
                    data-block-ssr-field="{{param.field.name}}"
                    data-block-ssr-group="{{../../../../group}}"
                    data-block-ssr-group-variant-index="-1"
                    data-block-ssr-group-variant-selector="{{param.field.name}}_{{@index}}_{{../../../../class}}"
                />?</span>{{/ifCond}}
            {{/with}}
        </div>
        {{/ifCond}}
        {{#with (first (lookup ../param prop.[0].list.[0]) (lookup ../../param prop.[0].list.[0])) as |param|}} <!-- ??? -->
            {{#each prop as |p|}}
                {{> contentSsrParamGroupVariantSection
                    class=../../../class
                    defaultName=../../../defaultName
                    field=param.field.name
                    group=../../../group
                    groupVariantSection=p
                    id=../../../id
                    index=@index
                    param=../../../param
                }}
            {{/each}}
        {{/with}}
    {{/each}}
</div>
