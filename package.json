{
  "name": "@akaterra.co/apidog",
  "version": "0.0.5",
  "description": "apiDog is a API documentation generator alternative to the apiDoc",
  "main": "start.js",
  "bin": "apidog",
  "dependencies": {
    "argparse": "1.0.10",
    "fast-sort": "1.5.6",
    "handlebars": "4.3.0",
    "htmldiff-js": "1.0.5",
    "lodash.get": "4.4.2",
    "markdown": "^0.5.0",
    "showdown": "^2.1.0",
    "uglify-js": "3.17.4"
  },
  "devDependencies": {
    "amqplib": "0.10.4",
    "amqplib-rpc": "3.1.0",
    "express": "4.19.2",
    "jasmine": "3.3.0",
    "nats": "2.23.0",
    "redis": "4.6.13",
    "socket.io": "4.7.5",
    "supertest": "7.0.0",
    "ws": "8.16.0"
  },
  "scripts": {
    "test": "NODE_ENV=test node $NODE_DEBUG_OPTION ./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
